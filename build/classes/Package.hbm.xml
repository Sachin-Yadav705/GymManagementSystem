<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">	
 
 <hibernate-mapping>

 <class name="pojo.Package" table="Package_table" schema="public">
   		<id name="packid" type="int">
   		<generator class="increment"></generator>
   	</id>
   	<property name="packname" column="name" type="string"></property>
   
   	<property name="packcategory" column="Category" type="string"></property>
   	<property name="packfee" column="Fee" type="int"></property>
   	<property name="packduration" column="Duration" type="int"></property>
   	<property name="packdescription" column="Description" type="string"></property>
   
   	</class> 
	</hibernate-mapping>